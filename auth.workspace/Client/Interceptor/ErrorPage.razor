@page "/ErrorPage/{statusCode}"
@page "/ErrorPage"
@inject ISnackbar Snackbar

<MudText>Server has responded as </MudText>@statusCode

@code {
    [Parameter]
    public string statusCode { get; set; }

    protected override async Task OnInitializedAsync(){
        AddSnackbar();
    }

    public void AddSnackbar()
    {
        Snackbar.Add($"{statusCode}", Severity.Normal);
    }

}
